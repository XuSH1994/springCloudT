<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sct.cn.mapper.TeacherMapper">

    <select id="selectTeacherByName" parameterType="String" resultType="com.sct.cn.vo.TeacherVO">
        select t.*
        from teacher t
        where t.name = '${name}'
    </select>
</mapper>
